<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" prefHeight="265.0" prefWidth="345.0"
            xmlns="http://javafx.com/javafx/8" fx:controller="controller.CreateNewAccountController">
    <children>
        <Label layoutX="37.0" layoutY="26.0" text="username:"/>
        <Label layoutX="38.0" layoutY="65.0" text="password:"/>
        <Label layoutX="14.0" layoutY="105.0" text="confirm password:"/>
        <Label layoutX="48.0" layoutY="149.0" text="E-mail:"/>
        <TextField fx:id="usernameField" layoutX="156.0" layoutY="22.0"/>
        <PasswordField fx:id="passwordField" layoutX="156.0" layoutY="61.0"/>
        <PasswordField fx:id="confirmPasswordField" layoutX="156.0" layoutY="101.0"/>
        <TextField fx:id="emailField" layoutX="156.0" layoutY="145.0"/>
        <Button fx:id="confirmButton" layoutX="90.0" layoutY="212.0" mnemonicParsing="false" onAction="#confirmHandler"
                text="confirm"/>
        <Button fx:id="cancelButton" layoutX="203.0" layoutY="212.0" mnemonicParsing="false" onAction="#cancelHandler"
                text="cancel"/>
        <Label fx:id="warningLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="156.0" layoutY="178.0"
               prefHeight="23.0" prefWidth="161.0" text="warningLabel" textFill="RED" visible="false"/>
    </children>
</AnchorPane>
