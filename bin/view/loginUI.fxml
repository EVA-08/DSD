<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" prefHeight="423.0" prefWidth="574.0"
            style="-fx-background-color: #444444;" xmlns="http://javafx.com/javafx/8"
            fx:controller="controller.LoginController">
    <children>
        <TextField fx:id="usernameField" layoutX="207.0" layoutY="132.0" style="-fx-background-color: #aaaaaa;"/>
        <Label layoutX="101.0" layoutY="136.0" style="-fx-text-fill: white;" text="username:"/>
        <Label layoutX="102.0" layoutY="212.0" style="-fx-text-fill: white;" text="password:"/>
        <Button fx:id="loginButton" layoutX="209.0" layoutY="280.0" mnemonicParsing="false" onAction="#loginHandler"
                onMouseEntered="#loginButtonOnMouseEntered" onMouseExited="#loginButtonOnMouseExited" prefHeight="66.0"
                prefWidth="161.0" text="login"/>
        <Hyperlink layoutX="223.0" layoutY="369.0" text="create new account"/>
        <PasswordField fx:id="passwordField" layoutX="209.0" layoutY="208.0" style="-fx-background-color: #aaaaaa;"/>
        <Label fx:id="warningLabel" layoutX="268.0" layoutY="250.0" text="Label"/>
    </children>
</AnchorPane>
